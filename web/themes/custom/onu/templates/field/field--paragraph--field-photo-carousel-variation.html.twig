    <button class="slider__navigation-btn right">
        <svg viewBox="0 0 15.74 22.99" class="carousel-gallery__svg">
            <use xlink:href="/themes/custom/onu/assets/svgs/icon-chevron.svg#icon"></use>
        </svg>
        <span class="access-hidden hide">Navigate Right</span></button>
</div>
