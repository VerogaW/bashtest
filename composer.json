        "drupal/google_analytics": "3.0.0",
        "drupal/google_cse": "3.1.0",
        "drupal/google_tag": "1.3.0",
        "drupal/group": "1.0-rc2",
        "drupal/groupmenu": "1.0.0-beta2",
        "drupal/inline_entity_form": "1.0.0-rc3",
        "drupal/libraries": "3.0-alpha1",
        "drupal/link_class": "1.4",
        "drupal/linked_field": "1.3.0",
        "drupal/linkit": "5.0-beta10",
        "drupal/mailsystem": "4.3.0",
        "drupal/masquerade": "2.0.0-beta3",
        "drupal/menu_block": "1.6.0",
