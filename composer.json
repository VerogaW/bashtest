        "drupal/ckeditor_media_embed": "1.9.0",
        "drupal/ckeditor_templates": "1.1.0",
        "drupal/coder": "8.3.8",
        "drupal/config_direct_save": "1.1.0",
        "drupal/config_installer": "^1.0",
        "drupal/config_partial_export": "1.6.0",
        "drupal/console": "^1",
        "drupal/core-recommended": "8.8.5",
        "drupal/crop": "2.1.0",
